<!DOCTYPE html>
<html>
  <head>
    <title>Aribeth</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body ng-app="aribeth">
    <%= render 'layouts/header' %>
    <div class="container-fluid" ng-view ng-controller="Global.Controller as gloc">
      <%= yield %>
    </div>
  </body>
</html>
